fmod VALUE is
    protecting NAT .
    sorts Value InitValue? Value? .
    subsort  Value < Value? .
    subsort InitValue? < Value? .

    subsort Nat < Value .

    op nilValue : -> InitValue? [ctor] .    

    sort ValueSet .
    subsort Value < ValueSet .    
    op emptyVs : -> ValueSet [ctor] .    
    op _;_ : ValueSet ValueSet -> ValueSet [ctor assoc comm id: emptyVs] .    
endfm